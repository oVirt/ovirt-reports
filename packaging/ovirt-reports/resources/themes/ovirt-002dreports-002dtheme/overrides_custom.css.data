.action.primary.over {
	/* gradient end values are #82aa32 to #f9771d with a top inner shadow */
	background-position: 0 0px;
	background-color: #82aa32;
}

.action.primary.pressed {
	/* gradient end values are #82aa32 to #f9771d with a top inner shadow */
	background-position: 0 0px;
	background-color: #749a28;
}


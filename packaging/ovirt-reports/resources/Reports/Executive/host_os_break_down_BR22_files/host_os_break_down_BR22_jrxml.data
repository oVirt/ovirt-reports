		<queryString language="SQL">
			<![CDATA[-- Datacenter aggregation level
--------
-- BR22 - Hosts Count by OS Type versus OS Version

SELECT
    history_datetime,
    COALESCE (
        v3_5_configuration_history_hosts.host_os,
        'Unknown'
    )
    AS os_type,
    COUNT ( DISTINCT v3_5_configuration_history_hosts.host_id )
    AS host_count
FROM  v3_5_statistics_hosts_resources_usage_$P!{table_name} AS stats_hosts
    INNER JOIN v3_5_configuration_history_hosts
        ON (
            v3_5_configuration_history_hosts.history_id =
            stats_hosts.host_configuration_version
        )
    INNER JOIN v3_5_configuration_history_hosts a
        ON (
            a.host_id =
            stats_hosts.host_id
        )
WHERE
    -- Here we filter by the datacenter chosen by the user
    v3_5_configuration_history_hosts.cluster_id IN (
        SELECT v3_5_configuration_history_clusters.cluster_id
        FROM v3_5_configuration_history_clusters
        WHERE
            v3_5_configuration_history_clusters.datacenter_id =
            CAST ( $P{P_DataCenter_ID} AS uuid )
    )
    -- Here we filter by the cluster chosen by the user
    AND v3_5_configuration_history_hosts.cluster_id =
        CASE $P{P_Cluster_ID}
            WHEN '11111111-1111-1111-1111-111111111111'
                THEN v3_5_configuration_history_hosts.cluster_id
            ELSE CAST ( $P{P_Cluster_ID} AS uuid )
        END
    AND history_datetime >= CAST ( $P{P_Start_Date} AS timestamp )
    AND history_datetime <
        CASE
            WHEN $P{P_Period} = 0
                THEN CAST ( $P{P_Start_Date} AS timestamp ) + interval '1 day'
            WHEN $P{P_Period} = 1
                THEN CAST ( $P{P_Start_Date} AS timestamp ) + interval '1 month'
            WHEN $P{P_Period} = 2
                THEN CAST ( $P{P_Start_Date} AS timestamp ) + interval '3 month'
            WHEN $P{P_Period} = 3
                THEN CAST ( $P{P_Start_Date} AS timestamp ) + interval '1 year'
        END
    -- Here we get the latest host configurations
    AND a.history_id IN (
        SELECT MAX ( b.history_id )
        FROM v3_5_configuration_history_hosts b
        GROUP BY b.host_id
    )
    -- Here we include or remove deleted hosts according to what
    -- the user chose in the "is_deleted" parameter.
    AND
        CASE
            WHEN $P{is_deleted} LIKE 'AND%'
                THEN a.delete_date IS NULL
            ELSE
                a.delete_date IS NULL
                OR
                a.delete_date IS NOT NULL
        END
GROUP BY
    history_datetime,
    COALESCE (
        v3_5_configuration_history_hosts.host_os,
        'Unknown'
    )]]>
		</queryString>

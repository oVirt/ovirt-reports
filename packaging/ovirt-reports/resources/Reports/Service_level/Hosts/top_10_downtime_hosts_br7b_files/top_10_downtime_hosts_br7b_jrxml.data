	<queryString language="SQL">
		<![CDATA[-- BR7B - The query will return the uptime; maintenance time and the down time,
-- for the bottom 10 hosts sorted by weighted uptime value descending.

SELECT
    v3_6_configuration_history_hosts.host_id,
    v3_6_configuration_history_hosts.host_name,
    CAST ( v3_6_configuration_history_hosts.delete_date AS date ) AS delete_date,
    (
        CASE
            WHEN v3_6_configuration_history_hosts.delete_date IS NULL
                THEN 0
            ELSE 1
        END
    )
    AS is_deleted,
    SUM (
        CASE
            WHEN stats_hosts.host_status = 2
                THEN
                    COALESCE (
                        stats_hosts.minutes_in_status,
                        0
                    )
            ELSE 0
        END
    )
    AS planned_downtime_mins,
    SUM (
        CASE
            WHEN stats_hosts.host_status = 3
                THEN
                    COALESCE (
                        stats_hosts.minutes_in_status,
                        0
                    )
            ELSE 0
        END
    )
    AS unplanned_downtime_mins,
    SUM (
        CASE
            WHEN stats_hosts.host_status = 1
                THEN
                    COALESCE (
                        stats_hosts.minutes_in_status,
                        0
                    )
            ELSE 0
        END
    )
    AS uptime_mins,
    SUM (
        stats_hosts.minutes_in_status
    ) AS total
-- If "Period" equals to "Daily" then "table_name"
-- parameter equals to "hourly" else "daily"
FROM v3_6_statistics_hosts_resources_usage_$P!{table_name} AS stats_hosts
    INNER JOIN v3_6_configuration_history_hosts
        ON (
            v3_6_configuration_history_hosts.host_id =
            stats_hosts.host_id
        )
WHERE
    -- Here we filter by the datacenter chosen by the user
    v3_6_configuration_history_hosts.cluster_id IN (
        SELECT cluster_id
        FROM v3_6_configuration_history_clusters
        WHERE
            datacenter_id = CAST ( $P{P_DataCenter_ID} AS uuid )
    )
    -- Here we filter by the cluster chosen by the user
    AND v3_6_configuration_history_hosts.cluster_id =
        CASE $P{P_Cluster_ID}
            WHEN NULL
                THEN v3_6_configuration_history_hosts.cluster_id
            WHEN '11111111-1111-1111-1111-111111111111'
                THEN v3_6_configuration_history_hosts.cluster_id
            ELSE CAST ( $P{P_Cluster_ID} AS uuid )
        END
    -- Here we filter by the host type chosen by the user
    AND v3_6_configuration_history_hosts.host_type =
        CASE $P{P_Host_Type}
            WHEN -1
                THEN v3_6_configuration_history_hosts.host_type
            ELSE $P{P_Host_Type}
        END
    -- Here we get the latest host configurations
    AND v3_6_configuration_history_hosts.history_id IN (
        SELECT MAX ( a.history_id )
        FROM v3_6_configuration_history_hosts a
        GROUP BY a.host_id
    )
    AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
    AND history_datetime <
        CASE
            WHEN $P{P_Period} = 0
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
            WHEN $P{P_Period} = 1
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
            WHEN $P{P_Period} = 2
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
            WHEN $P{P_Period} = 3
                THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
        END
    -- Here we get or remove deleted entities according to what the user chose
    -- in the "is_deleted" parameter.
    $P!{is_deleted}
GROUP BY
    v3_6_configuration_history_hosts.host_id,
    v3_6_configuration_history_hosts.host_name,
    (
        CASE
            WHEN v3_6_configuration_history_hosts.delete_date IS NULL
                THEN 0
            ELSE 1
        END
    ),
    v3_6_configuration_history_hosts.delete_date
HAVING
    ROUND (
        CAST (
            SUM (
                CASE
                    WHEN stats_hosts.host_status = 1
                        THEN
                            COALESCE (
                                stats_hosts.minutes_in_status,
                                0
                            )
                    ELSE 0
                END
            ) / 60
            AS float
        ) ::numeric ,
        1
    ) > 0
ORDER BY
    CAST (
        SUM (
            CASE
                WHEN stats_hosts.host_status = 3
                    THEN
                        COALESCE (
                            stats_hosts.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        )
        AS float
    ) /
    CAST (
        SUM (
            COALESCE (
                stats_hosts.minutes_in_status,
                0
            )
        )
        AS float
    ) DESC,
    is_deleted ASC
LIMIT 10]]>

	<queryString language="SQL">
		<![CDATA[-- BR6 - This query returns the cluster name and delete date
-- for the cluster chosen by the user.

SELECT DISTINCT
    cluster_name,
    delete_date
FROM v3_4_configuration_history_clusters
WHERE
    cluster_id = CAST ( $P{P_Cluster_ID} as uuid )
    AND history_id IN (
        SELECT MAX ( a.history_id )
        FROM v3_4_configuration_history_clusters as a
        GROUP BY a.cluster_id
    )]]>
	</queryString>

	<queryString language="SQL">
		<![CDATA[-- BR7A - This query returns the
-- average planned and unplanned downtime,
-- average uptime and total time per cluster.

SELECT
    cluster_id,
    name,
    delete_date,
    AVG ( planned_downtime_mins ) AS planned_downtime_mins,
    AVG ( unplanned_downtime_mins ) AS unplanned_downtime_mins,
    AVG ( uptime_mins ) AS uptime_mins,
    AVG ( total ) AS total
FROM (
    SELECT
        v3_6_configuration_history_hosts.cluster_id,
        v3_6_configuration_history_clusters.cluster_name AS name,
        v3_6_configuration_history_clusters.delete_date,
        v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_id,
        SUM (
            CASE
                WHEN v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_status = 2
                    THEN
                        COALESCE (
                            v3_6_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) AS planned_downtime_mins,
        SUM (
            CASE
                WHEN v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_status = 3
                    THEN
                        COALESCE (
                            v3_6_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) AS unplanned_downtime_mins,
        SUM (
            CASE
                WHEN v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_status = 1
                    THEN
                        COALESCE (
                            v3_6_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status,
                            0
                        )
                ELSE 0
            END
        ) AS uptime_mins,
        SUM (
            v3_6_statistics_hosts_resources_usage_$P!{table_name}.minutes_in_status
        ) AS total
    -- If "Period" equals to "Daily" then "table_name" parameter equals to "hourly" else "daily"
    FROM v3_6_statistics_hosts_resources_usage_$P!{table_name}
        INNER JOIN v3_6_configuration_history_hosts
            ON (
                v3_6_configuration_history_hosts.host_id =
                v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_id
            )
        INNER JOIN v3_6_configuration_history_clusters
            ON (
                v3_6_configuration_history_clusters.cluster_id =
                v3_6_configuration_history_hosts.cluster_id
            )
    WHERE
        -- Here we filter by the datacenter chosen by the user
        v3_6_configuration_history_hosts.cluster_id IN (
            SELECT cluster_id
             FROM v3_6_configuration_history_clusters
             WHERE datacenter_id = CAST ( $P{P_DataCenter_ID} AS uuid )
         )
        AND history_datetime >= CAST ( $P{P_Start_Date} AS TIMESTAMP )
        AND history_datetime <
            CASE
                WHEN $P{P_Period} = 0
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                WHEN $P{P_Period} = 1
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                WHEN $P{P_Period} = 2
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                WHEN $P{P_Period} = 3
                    THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
            END
        -- Here we get the latest clusters configuration
        AND v3_6_configuration_history_clusters.history_id IN (
            SELECT MAX ( a.history_id )
            FROM v3_6_configuration_history_clusters AS a
            GROUP BY a.cluster_id
        )
        -- Here we get the latest hosts configuration
        AND v3_6_configuration_history_hosts.history_id IN (
            SELECT MAX ( b.history_id )
            FROM v3_6_configuration_history_hosts AS b
            GROUP BY b.host_id
        )
        -- This will determine if deleted hosts will be included in the report,
        -- according to the user selection for "is_deleted" parameter
        AND
            CASE
                WHEN $P{is_deleted} LIKE 'AND%'
                    THEN v3_6_configuration_history_hosts.delete_date IS NULL
                ELSE
                    v3_6_configuration_history_hosts.delete_date IS NULL
                    OR
                    v3_6_configuration_history_hosts.delete_date IS NOT NULL
            END
    GROUP BY
        v3_6_configuration_history_hosts.cluster_id,
        v3_6_configuration_history_clusters.cluster_name,
        v3_6_configuration_history_clusters.delete_date,
        v3_6_statistics_hosts_resources_usage_$P!{table_name}.host_id
) AS a
GROUP BY
    cluster_id,
    name,
    delete_date
ORDER BY
    (
        CASE
            WHEN delete_date IS NULL
                THEN 0
            ELSE 1
        END
    ) ASC,
    CAST (
        AVG ( uptime_mins )
        AS float
    ) /
    CAST (
        AVG ( total )
        AS float
    ) DESC]]>
	</queryString>

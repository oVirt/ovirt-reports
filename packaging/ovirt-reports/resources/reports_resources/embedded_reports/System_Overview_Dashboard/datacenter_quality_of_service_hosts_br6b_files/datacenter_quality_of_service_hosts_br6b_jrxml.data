	<queryString language="SQL">
		<![CDATA[-- BR6B - This query returns the average
-- time above and below cpu threshold
-- and above and below memory threshold
-- per cluster.

SELECT *
FROM (
    SELECT
        cluster_id,
        cluster_name,
        is_delete,
        cluster_delete_only_date,
        AVG (
            time_above_cpu_threshold
        ) AS time_above_cpu_threshold,
        AVG (
            time_above_mem_threshold
        ) AS time_above_mem_threshold,
        AVG (
            time_below_cpu_threshold
        ) AS time_below_cpu_threshold,
        AVG (
            time_below_mem_threshold
        ) AS time_below_mem_threshold,
        AVG (
            overall_host_time
        ) AS overall_cluster_time
    FROM (
        SELECT
            v3_6_configuration_history_hosts.cluster_id,
            v3_6_configuration_history_clusters.cluster_name,
            (
                CASE
                    WHEN v3_6_configuration_history_clusters.delete_date IS NULL
                        THEN FALSE
                    ELSE TRUE
                END
            ) AS is_delete,
            v3_6_statistics_hosts_resources_usage_hourly.host_id,
            CAST (
                v3_6_configuration_history_clusters.delete_date
                AS date
            ) AS cluster_delete_only_date,
            SUM(
                CASE
                    WHEN cpu_usage_percent >= $P{P_CPU_Threshold}
                        THEN minutes_in_status
                    ELSE 0
                 END
             ) AS time_above_cpu_threshold,
            SUM (
                CASE
                    WHEN memory_usage_percent >= $P{P_MEM_Threshold}
                        THEN minutes_in_status
                    ELSE 0
                 END
             ) AS time_above_mem_threshold,
            SUM(
                CASE
                    WHEN cpu_usage_percent < $P{P_CPU_Threshold}
                        THEN minutes_in_status
                    ELSE 0
                 END
             ) AS time_below_cpu_threshold,
            SUM (
                CASE
                    WHEN memory_usage_percent < $P{P_MEM_Threshold}
                        THEN minutes_in_status
                    ELSE 0
                 END
             ) AS time_below_mem_threshold,
            SUM (
                COALESCE (
                    minutes_in_status,
                    0
                )
            ) AS overall_host_time
        FROM v3_6_statistics_hosts_resources_usage_hourly
            INNER JOIN v3_6_configuration_history_hosts
                ON (
                    v3_6_statistics_hosts_resources_usage_hourly.host_id =
                    v3_6_configuration_history_hosts.host_id
                )
            INNER JOIN v3_6_configuration_history_clusters
                ON (
                    v3_6_configuration_history_clusters.cluster_id =
                    v3_6_configuration_history_hosts.cluster_id
                )
        WHERE
            -- Here we get only active hosts
            v3_6_statistics_hosts_resources_usage_hourly.host_status = 1
            -- Here we filter by the datacenter chosen by the user
            AND v3_6_configuration_history_hosts.cluster_id IN (
                SELECT cluster_id
                FROM v3_6_configuration_history_clusters
                WHERE datacenter_id = CAST ( $P{P_DataCenter_ID} AS uuid )
            )
            AND v3_6_statistics_hosts_resources_usage_hourly.history_datetime >=
                CAST ($P{P_Start_Date} AS TIMESTAMP)
            AND v3_6_statistics_hosts_resources_usage_hourly.history_datetime <
                CASE
                    WHEN $P{P_Period} = 0
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 day'
                    WHEN $P{P_Period} = 1
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 month'
                    WHEN $P{P_Period} = 2
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '3 month'
                    WHEN $P{P_Period} = 3
                        THEN CAST ( $P{P_Start_Date} AS TIMESTAMP ) + interval '1 year'
                END
            -- Here we get the latest clusters configuration
            AND v3_6_configuration_history_clusters.history_id IN (
                SELECT MAX ( a.history_id )
                FROM v3_6_configuration_history_clusters a
                GROUP BY a.cluster_id
            )
            -- Here we get the latest hosts configuration
            AND v3_6_configuration_history_hosts.history_id IN (
                SELECT MAX ( b.history_id )
                FROM v3_6_configuration_history_hosts b
                GROUP BY b.host_id
            )
            -- This will determine if deleted hosts will be included in the report,
            -- according to the user selection for "is_deleted" parameter
            AND
                CASE
                    WHEN $P{is_deleted} LIKE 'AND%'
                        THEN v3_6_configuration_history_hosts.delete_date IS NULL
                    ELSE
                        v3_6_configuration_history_hosts.delete_date IS NULL
                        OR
                        v3_6_configuration_history_hosts.delete_date IS NOT NULL
                END
        GROUP BY    v3_6_configuration_history_hosts.cluster_id,
                v3_6_configuration_history_clusters.cluster_name,
                v3_6_configuration_history_clusters.delete_date,
                v3_6_statistics_hosts_resources_usage_hourly.host_id
    ) AS a
    GROUP BY
        cluster_id,
        cluster_name,
        is_delete,
        cluster_delete_only_date
) AS t
WHERE
    t.overall_cluster_time > 0
ORDER BY
    (
        CASE
            WHEN time_above_mem_threshold = 0 and time_above_cpu_threshold = 0
            THEN 1
            ELSE 0
        END
    ) ASC,
    t.is_delete ASC,
    (
        CAST (
            t.time_above_cpu_threshold AS float
        ) /
        CAST (
            t.overall_cluster_time AS float
        )
    ) *100 +
    (
        CAST (
            t.time_above_mem_threshold AS float
        ) /
        CAST (
            t.overall_cluster_time AS float
        )
    ) *100 DESC]]>
	</queryString>

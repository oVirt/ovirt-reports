    <queryString>
        <![CDATA[SELECT v3_0_latest_host_configuration_view.host_id, v3_0_latest_host_configuration_view.host_name, AVG(cpu_usage_percent) AS cpu_usage_percent_avg, MAX(cpu_usage_percent) AS cpu_usage_percent_max, AVG(memory_usage_percent) AS memory_usage_percent_avg, MAX(memory_usage_percent) AS memory_usage_percent_max
FROM v3_0_host_samples_history_view
    INNER JOIN v3_0_latest_host_configuration_view ON (v3_0_latest_host_configuration_view.host_id = v3_0_host_samples_history_view.host_id)
WHERE v3_0_host_samples_history_view.host_status = 1
      AND v3_0_latest_host_configuration_view.cluster_id = cast($P{P_Cluster_ID} as UUID)
GROUP BY v3_0_latest_host_configuration_view.host_id, v3_0_latest_host_configuration_view.host_name
ORDER BY AVG(cpu_usage_percent) + MAX(cpu_usage_percent) + AVG(memory_usage_percent) + MAX(memory_usage_percent) DESC
LIMIT 5]]>

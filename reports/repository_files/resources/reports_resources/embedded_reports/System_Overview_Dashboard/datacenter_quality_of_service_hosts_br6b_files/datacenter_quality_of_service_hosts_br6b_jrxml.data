    <queryString>
        <![CDATA[SELECT *
FROM   (SELECT     cluster_id,
        cluster_name,
        is_delete,
        cluster_delete_only_date,
        avg(time_above_cpu_threshold) as time_above_cpu_threshold,
        avg(time_above_mem_threshold) as time_above_mem_threshold,
        avg(time_below_cpu_threshold) as time_below_cpu_threshold,
        avg(time_below_mem_threshold) as time_below_mem_threshold,
        avg(overall_host_time) as overall_cluster_time
    FROM    (SELECT v3_2_configuration_history_hosts.cluster_id,
            v3_2_configuration_history_clusters.cluster_name,
            (CASE WHEN v3_2_configuration_history_clusters.delete_date IS NULL THEN FALSE ELSE TRUE END) as is_delete,
            v3_2_statistics_hosts_resources_usage_hourly.host_id,
            CAST(v3_2_configuration_history_clusters.delete_date as date) as cluster_delete_only_date,
            SUM(CASE
                WHEN cpu_usage_percent >= $P{P_CPU_Threshold} THEN minutes_in_status
                ELSE 0
                 END) as time_above_cpu_threshold,
            SUM (CASE
                WHEN memory_usage_percent >= $P{P_MEM_Threshold} THEN minutes_in_status
                ELSE 0
                 END) as time_above_mem_threshold,
            SUM(CASE
                WHEN cpu_usage_percent < $P{P_CPU_Threshold} THEN minutes_in_status
                ELSE 0
                 END) as time_below_cpu_threshold,
            SUM (CASE
                WHEN memory_usage_percent < $P{P_MEM_Threshold} THEN minutes_in_status
                ELSE 0
                 END) as time_below_mem_threshold,
            SUM(coalesce(minutes_in_status, 0)) as overall_host_time
        FROM v3_2_statistics_hosts_resources_usage_hourly
            INNER JOIN v3_2_configuration_history_hosts
                ON (v3_2_statistics_hosts_resources_usage_hourly.host_id = v3_2_configuration_history_hosts.host_id)
                    INNER JOIN v3_2_configuration_history_clusters
                        ON (v3_2_configuration_history_clusters.cluster_id = v3_2_configuration_history_hosts.cluster_id)
        WHERE    v3_2_statistics_hosts_resources_usage_hourly.host_status = 1
            AND v3_2_configuration_history_hosts.cluster_id in (SELECT cluster_id
                                        FROM v3_2_configuration_history_clusters
                                    WHERE datacenter_id = cast($P{P_DataCenter_ID} as uuid))
            AND v3_2_statistics_hosts_resources_usage_hourly.history_datetime >= cast($P{P_Start_Date} as timestamp)
            AND v3_2_statistics_hosts_resources_usage_hourly.history_datetime <= CASE
                                                                     WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                                     WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                                     WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                                     WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                                 END
            AND v3_2_configuration_history_clusters.history_id in (SELECT max(a.history_id)
                                      FROM v3_2_configuration_history_clusters a
                                      GROUP BY a.cluster_id)
            AND v3_2_configuration_history_hosts.history_id in (SELECT max(b.history_id)
                                    FROM v3_2_configuration_history_hosts b
                                    GROUP BY b.host_id)
            AND CASE
                WHEN $P{is_deleted} like 'AND%' THEN v3_2_configuration_history_hosts.delete_date IS NULL
                ELSE v3_2_configuration_history_hosts.delete_date IS NULL or v3_2_configuration_history_hosts.delete_date IS NOT NULL
                END
        GROUP BY    v3_2_configuration_history_hosts.cluster_id,
                v3_2_configuration_history_clusters.cluster_name,
                v3_2_configuration_history_clusters.delete_date,
                v3_2_statistics_hosts_resources_usage_hourly.host_id) a
    GROUP BY cluster_id,
         cluster_name,
         is_delete,
         cluster_delete_only_date) t
WHERE t.overall_cluster_time > 0
ORDER BY (CASE WHEN time_above_mem_threshold = 0 and time_above_cpu_threshold = 0 THEN 1 ELSE 0 END) ASC, t.is_delete ASC, (cast(t.time_above_cpu_threshold as float)/cast(t.overall_cluster_time as float))*100 + (cast(t.time_above_mem_threshold as float)/cast(t.overall_cluster_time as float))*100 DESC]]>
    </queryString>

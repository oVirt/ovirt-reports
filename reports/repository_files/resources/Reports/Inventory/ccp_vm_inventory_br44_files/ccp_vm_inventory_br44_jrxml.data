        <queryString>
            <![CDATA[SELECT v3_4_configuration_history_vms.cluster_id
      ,v3_4_configuration_history_clusters.cluster_name
      ,v3_4_configuration_history_clusters.delete_date as cluster_delete
      ,coalesce(b.value_localized,b.value) AS os_version
      ,v3_4_configuration_history_vms.vm_name
      ,v3_4_configuration_history_vms.vm_description
      ,v3_4_configuration_history_vms.template_name
      ,v3_4_configuration_history_vms.create_date
      ,v3_4_configuration_history_vms.delete_date
      ,v3_4_configuration_history_vms.number_of_sockets
      ,v3_4_configuration_history_vms.cpu_per_socket
      ,coalesce(cpu_per_socket, 0) * coalesce(number_of_sockets, 0) AS vm_cpucores
      ,ROUND(CAST(EXTRACT(epoch FROM coalesce(v3_4_configuration_history_vms.delete_date,CURRENT_TIMESTAMP) - v3_4_configuration_history_vms.create_date)/3600 as numeric), 2) as existence_time_hours
      ,coalesce(round(sum(CASE
                        WHEN minutes_in_status IS NOT NULL THEN minutes_in_status
                        ELSE 0
                    END)/60,2),0) as vm_uptime
      ,v3_4_configuration_history_vms.vm_id
FROM v3_4_configuration_history_vms
         LEFT OUTER JOIN (SELECT enum_translator_default.enum_type, enum_translator_default.enum_key, enum_translator_default.value, enum_translator_localized.value_localized
                          FROM enum_translator as enum_translator_default
                                   LEFT OUTER JOIN (SELECT enum_type, enum_key, value as value_localized
                                                    FROM enum_translator
                                                    WHERE language_code = $P{P_Locale})  as enum_translator_localized ON (enum_translator_localized.enum_type = enum_translator_default.enum_type AND enum_translator_localized.enum_key = enum_translator_default.enum_key)
                          WHERE language_code = 'en_US') as b ON (v3_4_configuration_history_vms.operating_system = b.enum_key AND b.enum_type = 'OS_TYPE')
         LEFT OUTER JOIN v3_4_configuration_history_clusters ON (v3_4_configuration_history_vms.cluster_id = v3_4_configuration_history_clusters.cluster_id
                                                             AND v3_4_configuration_history_clusters.history_id in (SELECT max(a.history_id)
                                                                                                                    FROM v3_4_configuration_history_clusters a
                                                                                                                    GROUP BY a.cluster_id))
         LEFT OUTER JOIN v3_4_statistics_vms_resources_usage_$P!{table_name} as c ON (v3_4_configuration_history_vms.vm_id = c.vm_id
                                                                                      AND c.vm_status = 1
                                                                                      AND c.history_datetime >= cast($P{P_Start_Date} as timestamp)
                                                                                      AND c.history_datetime < CASE
                                                                                                                    WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                                                                                    WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                                                                                    WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                                                                                    WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                                                                                END)
WHERE v3_4_configuration_history_vms.cluster_id in (SELECT cluster_id
                                                    FROM v3_4_configuration_history_clusters
                                                    WHERE datacenter_id = cast($P{P_DataCenter_ID} as uuid))
      AND v3_4_configuration_history_vms.cluster_id = CASE cast($P{P_Cluster_ID} as uuid)
                                                          WHEN NULL THEN v3_4_configuration_history_vms.cluster_id
                                                          WHEN CAST('11111111-1111-1111-1111-111111111111' as uuid) THEN v3_4_configuration_history_vms.cluster_id
                                                          ELSE cast($P{P_Cluster_ID} as uuid)
                                                       END
      AND v3_4_configuration_history_vms.vm_type = CASE $P{P_VM_Type}
                                                       WHEN -1 THEN v3_4_configuration_history_vms.vm_type
                                                       ELSE $P{P_VM_Type}
                                                   END
      AND v3_4_configuration_history_vms.history_id in (SELECT max(a.history_id)
                                                        FROM v3_4_configuration_history_vms a
                                                        GROUP BY a.vm_id)
      AND CASE
              WHEN $P{is_deleted} like '%delete%' THEN v3_4_configuration_history_vms.delete_date IS NULL
              ELSE v3_4_configuration_history_vms.delete_date IS NULL or v3_4_configuration_history_vms.delete_date IS NOT NULL
          END
      AND v3_4_configuration_history_vms.create_date < CASE
                                                           WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                           WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                           WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                           WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                       END
      AND ((v3_4_configuration_history_vms.delete_date < CASE
                                                           WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                           WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                           WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                           WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                       END
            AND v3_4_configuration_history_vms.delete_date >= cast($P{P_Start_Date} as timestamp)) OR v3_4_configuration_history_vms.delete_date IS NULL)
GROUP BY v3_4_configuration_history_vms.cluster_id
         ,v3_4_configuration_history_clusters.cluster_name
         ,v3_4_configuration_history_clusters.delete_date
         ,coalesce(b.value_localized,b.value)
         ,v3_4_configuration_history_vms.vm_name
         ,v3_4_configuration_history_vms.vm_description
         ,v3_4_configuration_history_vms.template_name
         ,v3_4_configuration_history_vms.create_date
         ,v3_4_configuration_history_vms.delete_date
         ,v3_4_configuration_history_vms.number_of_sockets
         ,v3_4_configuration_history_vms.cpu_per_socket
         ,coalesce(cpu_per_socket, 0) * coalesce(number_of_sockets, 0)
         ,ROUND(CAST(EXTRACT(epoch FROM coalesce(v3_4_configuration_history_vms.delete_date,CURRENT_TIMESTAMP) - v3_4_configuration_history_vms.create_date)/3600 as numeric), 2)
         ,v3_4_configuration_history_vms.vm_id
ORDER BY v3_4_configuration_history_vms.cluster_id, coalesce(b.value_localized,b.value), v3_4_configuration_history_vms.delete_date DESC, vm_uptime DESC, existence_time_hours DESC]]>
        </queryString>

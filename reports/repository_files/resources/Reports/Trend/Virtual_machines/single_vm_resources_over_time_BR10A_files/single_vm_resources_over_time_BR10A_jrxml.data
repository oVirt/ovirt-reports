                                        WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                    END
GROUP BY $P!{calendar_column}, interface_name
ORDER BY $P!{calendar_column} ASC]]>
        </queryString>

        <queryString>
            <![CDATA[select distinct $P!{calendar_column}  as the_date, coalesce(interface_name,max(interface_name) over (partition by 1), '') as interface_name, avg(Rx_rate) as Rx_rate, avg(Tx_rate) as Tx_rate
from    (-- VMs Ethernet Interface Tx and Rx Rates
         SELECT v3_2_configuration_history_vms_interfaces.vm_interface_id,
                CASE
                    WHEN delete_date IS NULL THEN v3_2_configuration_history_vms_interfaces.vm_interface_name
                    ELSE v3_2_configuration_history_vms_interfaces.vm_interface_name || '*'
                END as interface_name,
                history_datetime AS time,
                avg(coalesce(receive_rate_percent,0)) AS Rx_rate,
                avg(coalesce(transmit_rate_percent,0)) AS Tx_rate
         FROM v3_2_statistics_vms_interfaces_resources_usage_$P!{table_name}
              INNER JOIN v3_2_configuration_history_vms_interfaces ON (v3_2_configuration_history_vms_interfaces.vm_interface_id = v3_2_statistics_vms_interfaces_resources_usage_$P!{table_name}.vm_interface_id)
         WHERE history_datetime >= cast($P{P_Start_Date} as timestamp)
               AND history_datetime <= CASE
                                           WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                           WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                           WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                           WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                       END
               AND v3_2_configuration_history_vms_interfaces.history_id in (SELECT max(a.history_id)
                                                                            FROM v3_2_configuration_history_vms_interfaces as a
                                                                            GROUP BY a.vm_interface_id)
               $P!{is_deleted}
               AND v3_2_configuration_history_vms_interfaces.vm_interface_id in (SELECT c.vm_interface_id
                                                                                 FROM v3_2_statistics_vms_interfaces_resources_usage_$P!{table_name} b
                                                                                      INNER JOIN v3_2_configuration_history_vms_interfaces c ON (c.vm_interface_id = b.vm_interface_id)
                                                                                 WHERE c.vm_interface_id in (SELECT device_id
                                                                                                             FROM v3_2_configuration_history_vms_devices
                                                                                                             WHERE vm_id = CASE $P{P_VM_ID}
                                                                                                                               WHEN '00000000-0000-0000-0000-000000000000' THEN '00000000-0000-0000-0000-000000000000'
                                                                                                                               WHEN null THEN '00000000-0000-0000-0000-000000000000'
                                                                                                                               WHEN '11111111-1111-1111-1111-111111111111' THEN '00000000-0000-0000-0000-000000000000'
                                                                                                                               ELSE cast($P{P_VM_ID} as uuid)
                                                                                                                           END
                                                                                                                   AND create_date <= CASE
                                                                                                                                          WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                                                                                                          WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                                                                                                          WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                                                                                                          WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                                                                                                      END
                                                                                                                   AND (delete_date <= CAST($P{P_Start_Date} as TIMESTAMP) OR delete_date IS NULL)
                                                                                                                   AND history_id in (SELECT max(a.history_id)
                                                                                                                                      FROM v3_2_configuration_history_vms_devices a
                                                                                                                                      GROUP BY a.vm_id, a.device_id)
                                                                                                                   $P!{is_deleted})
                                                                                       AND history_datetime >= cast($P{P_Start_Date} as timestamp)
                                                                                       AND history_datetime <= CASE
                                                                                                                   WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                                                                                                   WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
                                                                                                                   WHEN $P{P_Period} = 2 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '3 month'
                                                                                                                   WHEN $P{P_Period} = 3 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 year'
                                                                                                               END
                                                                                       AND c.history_id in (SELECT max(d.history_id)
                                                                                                            FROM v3_2_configuration_history_vms_interfaces as d
                                                                                                            GROUP BY d.vm_interface_id)
                                                                                       $P!{is_deleted}
                                                                                 GROUP BY c.vm_interface_id
                                                                                 ORDER BY avg(coalesce(b.receive_rate_percent,0)) + avg(coalesce(b.transmit_rate_percent,0)) DESC
                                                                                 LIMIT 3)
         Group BY v3_2_configuration_history_vms_interfaces.vm_interface_id, interface_name, history_datetime) as a RIGHT OUTER JOIN calendar as b ON (a.time = $P!{calendar_column})
WHERE b.$P!{calendar_column} >= cast($P{P_Start_Date} as timestamp)
      AND b.$P!{calendar_column} <= CASE
                                        WHEN $P{P_Period} = 0 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 day'
                                        WHEN $P{P_Period} = 1 THEN CAST($P{P_Start_Date} as TIMESTAMP) + interval '1 month'
